{
    "name": "quadrogod/laravel-mobizon",
    "description": "Mobizon SMS notifications driver for Laravel",
    "keywords": [
        "laravel",
        "notifications",
        "mobizon",
        "sms",
        "php"
    ],
    "homepage": "https://github.com/quadrogod/laravel-mobizon",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Molchanov",
            "homepage": "https://github.com/quadrogod",
            "role": "Developer"
        }
    ],
    "require": {
        "lib-curl": "*",
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.2",
        "mobizon/mobizon-php": "~2.0"

    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "Quadrogod\\LaravelMobizon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Quadrogod\\LaravelMobizon\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
